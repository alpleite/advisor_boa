
<div class="row wrapper border-bottom white-bg page-heading">
	<div class="col-lg-10">
		<h2>Cadastro de Lotes</h2>
	</div>
	<div class="col-lg-2">

	</div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
	<div class="row">
		<div class="col-lg-12">
			<div class="ibox">
				<div class="ibox-title">
					<h5>Wizard criação de lote</h5>
				</div>
				<div class="ibox-content">
					<div class="wizard-big wizard clearfix" role="application" novalidate="novalidate">
						<div class="steps clearfix">
							<ul role="tablist">
								<li role="tab" class="first current" aria-disabled="false" aria-selected="true">
									<a aria-controls="form-p-0" href="#form-h-0" id="form-t-0">
										<span class="current-info audible">current step: </span>
										<span class="number">1.</span> Criação do lote
									</a>
								</li>
								<li role="tab" class="disabled" aria-disabled="true">
									<a aria-controls="form-p-1" href="#" id="form-t-1">
										<span class="number">2.</span> Envio de Arquivos PDF
									</a>
								</li>
								<li role="tab" class="disabled" aria-disabled="true">
									<a aria-controls="form-p-2" href="#" id="form-t-2">
										<span class="number">3.</span>Processamento
									</a>
								</li>
							</ul>
						</div>
						<div class="content clearfix">		
							<h1 id="form-h-0" tabindex="-1" class="title current">Lote</h1>
							<fieldset id="form-p-0" role="tabpanel" aria-labelledby="form-h-0" class="body current" aria-hidden="false">
								<h2>Início criação do Lote</h2>
								<div class="row">
									<div class="col-lg-8">
										<%= render 'form' %>
									</div>
								</div>
							</fieldset>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<script type="text/javascript">
	$( document ).ready(function() {
    	$("#allotment_consumers_id").addClass( "form-control" );
	});
</script>
